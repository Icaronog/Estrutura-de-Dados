#include <stdio.h>


int main(){

    int linha, coluna;


    printf("digite o numero de linhas: ");
    scanf("%d", &linha);
    printf("digite o numero de colunas: ");
    scanf("%d", &coluna);

    int matriz[linha][coluna];

    for(int i = 0; i<=linha-1; i++){
        for(int j=0; j<=coluna-1; j++){
            matriz[i][j] = 1;
        }
    }

    for(int i = 0; i<=linha-1; i++){
        for(int j=0; j<=coluna-1; j++){
            printf("%d", matriz[i][j]);
        }
        printf("\n");
    }

    return 0;
}
